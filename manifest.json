{"manifest_version" : 2
,"name": "The Old Reader"
,"version" : "1.1"
,"developer": {"name" : "Lazare INEPOLOGLOU", "url" : "http://linepogl.wordpress.com"}
,"description" : "Notifies you about unread items in your The Old Reader account."
,"background" : {"scripts" :["index.js"]}
,"icons" : {"16": "icon-16.png","48": "icon-48.png","64": "icon-64.png","128": "icon-128.png"}
,"browser_action": { "default_icon": "icon-inactive.png", "default_title": "The Old Reader" }
,"web_accessible_resources" : ["icon-128.png","icon-16.png","icon-48.png","icon-active.png","icon-inactive.png","licence.txt"]
,"permissions" : ["tabs","*://theoldreader.com/"]
}
